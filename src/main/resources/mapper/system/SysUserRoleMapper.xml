<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.geekidea.boot.system.mapper.SysUserRoleMapper">

    <select id="getSysUserRoleById" resultType="io.geekidea.boot.system.vo.SysUserRoleInfoVo">
        select *
        from sys_user_role
        where id = #{id}
    </select>

    <select id="getSysUserRoleList" parameterType="io.geekidea.boot.system.query.SysUserRoleQuery"
            resultType="io.geekidea.boot.system.vo.SysUserRoleVo">
        select *
        from sys_user_role
    </select>

</mapper>
